    "start:queue:dev": "ts-node-dev src/worker.ts",
    "start:dev": "ts-node-dev src/main.ts",
    "start:dev:debug": "node --trace-warnings -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "db:seed": "prisma db seed --preview-feature",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
